{
  "name": "reactpress",
  "version": "0.0.1",
  "description": "WordPress, Docker, Webpack, Win.",
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./scripts/start.js",
    "build": "cross-env NODE_ENV=production node ./scripts/build.js",
    "postinstall": "npm run build",
    "start:docker": "cd docker && docker-compose up -d && cd ../",
    "stop:docker": "cd docker && docker-compose down && cd ../",
    "dump-db": "docker exec -i docker_db_1 /usr/bin/mysqldump -u wordpress -pwordpress wordpress > ./docker/backup.sql",
    "restore-db": "docker exec -i docker_db_1 /usr/bin/mysql -u wordpress -pwordpress wordpress < ./docker/backup.sql"
  },
  "author": "jaredpalmer",
  "license": "MIT",
  "dependencies": {
    "jquery": "^3.4.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "webpack": "^4.29.6"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.0.0",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@svgr/webpack": "4.1.0",
    "assets-webpack-plugin": "^3.9.7",
    "autoprefixer": "^9.4.9",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^7.0.1",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.16.4",
    "sass-loader": "^7.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.2",
    "webpack-cli": "^3.2.3"
  }
}
